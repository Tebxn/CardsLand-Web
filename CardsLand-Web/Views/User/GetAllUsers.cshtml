@model IEnumerable<CardsLand_Web.Entities.UserEnt>
@{
	ViewData["Title"] = "GetAllUsers";
}

<!doctype html>
	<body>
	<section class="ftco-section">
		<div class="container">
			<div class="row justify-content-center">
				<div class="col-md-6 text-center mb-5">
					<h2 class="heading-section">PokeTrainers!</h2>
				</div>
			</div>
			<div class="row">
				<div class="col-md-12">
					<div class="table-wrap">
						<table class="table table-responsive-xl">
						  <thead>
						    <tr>
						    	<th>Nickname</th>
								<th>Correo Electronico</th>
								<th>Estado</th>
						    </tr>
						  </thead>
						  <tbody>
							  @foreach(var x in Model){
						    <tr>
										<td>@x.User_Nickname</td>
										<td>@x.User_Email</td>
										<td>@x.User_State</td>
						    
						    </tr>
							}
						  </tbody>
						</table>
					</div>
				</div>
			</div>
		</div>
	</section>

	</body>
</html>
